    "activityBarBadge.background": "#bc2823",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#928fea",
    "statusBar.background": "#6864e2",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#928fea",
    "statusBarItem.remoteBackground": "#6864e2",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#6864e2",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#6864e299",
    "titleBar.inactiveForeground": "#e7e7e799"
